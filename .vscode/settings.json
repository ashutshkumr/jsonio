{
    "editor.rulers": [
        88,
        90
    ],
    "files.insertFinalNewline": true,
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/helpers/dut/gnmi/**": true
    },
    "files.eol": "\n",
    "diffEditor.ignoreTrimWhitespace": false,
    "files.associations": {
        "*.excalidraw": "json",
        "*.excalidrawlib": "json",
        "iostream": "cpp"
    },
}
